{ 
    "manifest_version": 2,
    "name": "20/20/20", 
    "description": "Reminder to look at an object 20 feet away for 20 seconds every 20 minutes of screentime!",
    "version": "1.0.0",
    "icons": {
        "128": "images/20_logo.png"
    },
    "browser_action": {
        "default_icon": "images/20_logo.png",
        "default_popup": "popup.html"   
     },
    "background": {
        "persistent": false,
        "scripts": ["popup.js"]
    },
    "permissions": [
        "activeTab",
        "background"
    ]
}